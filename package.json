{
  "name": "maperture",
  "author": "Stamen Design <info@stamen.com> (https://stamen.com)",
  "description": "Stamen's map compare tool",
  "version": "0.16.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stamen/maperture.git"
  },
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "build-package": "rm -rf dist && rollup -c rollup.config.package.js",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "husky install",
    "deploy": "yarn build && node scripts/deploy.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^7.0.0",
    "prettier": "^2.6.0",
    "prettier-plugin-svelte": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@beyonk/svelte-mapbox": "^8.1.4",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@googlemaps/js-api-loader": "^1.13.10",
    "autoprefixer": "^10.4.14",
    "deep-equal": "^2.0.5",
    "hat": "^0.0.3",
    "html2canvas": "^1.4.1",
    "leaflet": "^1.9.3",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "mapbox-gl": "^2.14.1",
    "maplibre-gl": "^3.6.2",
    "postcss": "^8.4.23",
    "sirv-cli": "^1.0.0",
    "stamen-attribution": "github:stamen/stamen-attribution#v0.1.0",
    "svelte-fa": "^2.4.0",
    "tangram": "^0.21.1"
  },
  "main": "dist/bundle.js"
}
